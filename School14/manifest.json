{
  "name": "School 14",
  "version": "4.0",
  "description": "by Araslanov Agzam",
  "manifest_version": 3,
  "content_scripts": [
  {
    "matches": ["https://vk.com/*", "file:///*"],
    "js": ["run.js"],
    "css": ["run.css"],
    "run_at": "document_end"
  }],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "action": {
		"default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
        "https://*/"
  ]
}
